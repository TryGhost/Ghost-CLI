{
  "name": "ghost-cli",
  "version": "1.10.0",
  "description": "CLI Tool for installing & updating Ghost",
  "author": "Ghost Foundation",
  "homepage": "https://ghost.org",
  "keywords": [
    "ghost",
    "cli"
  ],
  "files": [
    "bin",
    "lib",
    "extensions",
    "yarn.lock"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TryGhost/Ghost-CLI/"
  },
  "bugs": "https://github.com/TryGhost/Ghost-CLI/issues",
  "contributors": "https://github.com/TryGhost/Ghost-CLI/graphs/contributors",
  "license": "MIT",
  "main": "lib/index.js",
  "bin": {
    "ghost": "./bin/ghost"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint bin/* lib test extensions",
    "test:unit": "nyc --reporter=html --reporter=text mocha --recursive test/unit extensions/**/test",
    "test:acceptance": "mocha --timeout 10000 test/acceptance/**/*-spec.js",
    "test:all": "yarn test:unit && yarn test:acceptance",
    "test": "yarn test:all",
    "posttest": "yarn lint",
    "ship": "f() { STATUS=$(git status --porcelain); echo $STATUS; if [ -z \"$STATUS\" ]; then standard-version $@ && git push --follow-tags;  fi; }; f"
  },
  "nyc": {
    "exclude": [
      "**/*-spec.js",
      "test"
    ]
  },
  "engines": {
    "node": "^8.9.0 || ^10.13.0"
  },
  "preferGlobal": true,
  "dependencies": {
    "abbrev": "1.1.1",
    "adm-zip": "0.4.13",
    "bluebird": "3.5.4",
    "chalk": "2.4.2",
    "cli-table3": "0.5.0",
    "debug": "4.1.1",
    "decompress": "4.2.0",
    "download": "7.1.0",
    "execa": "1.0.0",
    "find-plugins": "1.1.7",
    "fkill": "5.3.0",
    "fs-extra": "7.0.1",
    "generate-password": "1.4.1",
    "global-modules": "2.0.0",
    "got": "8.3.2",
    "inquirer": "6.3.1",
    "is-running": "2.1.0",
    "latest-version": "4.0.0",
    "listr": "0.14.3",
    "lodash": "4.17.11",
    "log-symbols": "2.2.0",
    "moment": "2.24.0",
    "mysql": "2.17.1",
    "ora": "3.4.0",
    "package-json": "5.0.0",
    "path-is-root": "0.1.0",
    "portfinder": "1.0.20",
    "prettyjson": "1.2.1",
    "read-last-lines": "1.7.1",
    "replace-in-file": "4.0.0",
    "rxjs": "6.5.1",
    "semver": "6.0.0",
    "shasum": "1.0.2",
    "stat-mode": "0.3.0",
    "strip-ansi": "5.2.0",
    "symlink-or-copy": "1.2.0",
    "systeminformation": "4.1.6",
    "tail": "2.0.2",
    "validator": "7.2.0",
    "yargs": "12.0.5",
    "yarn": "1.15.2"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "coveralls": "3.0.3",
    "eslint": "5.16.0",
    "eslint-plugin-ghost": "0.2.0",
    "has-ansi": "3.0.0",
    "mocha": "6.1.4",
    "nyc": "14.1.1",
    "proxyquire": "2.1.0",
    "sinon": "7.3.2",
    "standard-version": "4.3.0",
    "tmp": "0.1.0"
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  }
}
