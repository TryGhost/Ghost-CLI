{
  "name": "ghost-cli",
  "version": "1.0.0-alpha.16",
  "description": "Just a CLI manager (for a blogging platform)",
  "author": "Ghost Foundation",
  "homepage": "http://ghost.org",
  "keywords": [
    "ghost",
    "cli"
  ],
  "files": [
    "bin",
    "lib",
    "yarn.lock"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TryGhost/Ghost-CLI/"
  },
  "bugs": "https://github.com/TryGhost/Ghost-CLI/issues",
  "contributors": "https://github.com/TryGhost/Ghost-CLI/graphs/contributors",
  "license": "MIT",
  "main": "lib/cli.js",
  "bin": {
    "ghost": "./bin/ghost"
  },
  "scripts": {
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint bin lib",
    "test:unit": "istanbul cover _mocha --report lcovonly -- test/unit/**/*-spec.js",
    "test:acceptance": "mocha --timeout 10000 test/acceptance/**/*-spec.js",
    "test:all": "yarn run test:unit && yarn run test:acceptance",
    "test": "yarn run lint && yarn run test:all"
  },
  "engines": {
    "node": "^4.5.0 || ^6.5.0"
  },
  "preferGlobal": true,
  "dependencies": {
    "bluebird": "3.5.0",
    "chalk": "1.1.3",
    "cli-table2": "0.2.0",
    "commander": "2.9.0",
    "debug": "2.6.6",
    "decompress": "4.1.0",
    "download": "6.0.0",
    "execa": "0.6.3",
    "fkill": "4.1.0",
    "fs-extra": "3.0.1",
    "ghost-ignition": "2.8.11",
    "greenlock-cli": "2.2.7",
    "inquirer": "3.0.6",
    "knex-migrator": "2.0.16",
    "listr": "0.12.0",
    "lodash": "4.17.4",
    "nginx-conf": "1.3.0",
    "ora": "1.2.0",
    "portfinder": "1.0.13",
    "rxjs": "5.3.1",
    "semver": "5.3.0",
    "shasum": "1.0.2",
    "slice-file": "1.0.0",
    "strip-ansi": "3.0.1",
    "symlink-or-copy": "1.1.8",
    "update-notifier": "2.1.0",
    "validator": "7.0.0",
    "yarn": "0.23.4"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "coveralls": "2.13.1",
    "eslint": "3.19.0",
    "istanbul": "0.4.5",
    "mocha": "3.3.0",
    "mocha-lcov-reporter": "1.3.0",
    "proxyquire": "1.7.11",
    "rewire": "2.5.2",
    "sinon": "2.2.0",
    "tmp": "0.0.31"
  }
}
